// 1978 소수찾기

#define _CRT_SECURE_NO_WARNINGS

#include <iostream>
#include <string>
using namespace std;

int sosu = 0;

int main()
{

	int N;
	cin >> N;
	for (int i = 0; i < N; i++)
	{
		int a;
		cin >> a;

		int cur = 2;
		int count = 0;
		while (cur <= a)
		{
			if (a%cur == 0)
				count++;
			cur++;
		}
		if (count == 1)
			sosu++;



	}
	cout << sosu << "\n";

}
